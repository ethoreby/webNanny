{
  "name": "Web Nanny",
  "description": "Filter profanity in webpages",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["run.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Clean this page!"
  },
  "manifest_version": 2
}
