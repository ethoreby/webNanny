{
  "name": "Web Nanny",
  "description": "Replace bad language on your webpage",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["run.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Remove bad language!"
  },
  "manifest_version": 2
}
